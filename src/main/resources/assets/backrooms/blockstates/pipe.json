{
    "multipart": [
        {   
            "apply": { "model": "backrooms:block/pipe/middle" }
        },
		
        {   "when": { "down": "true" },
            "apply": { "model": "backrooms:block/pipe/side", "x": 90, "uvlock": false }
        },
		
        {   "when": { "up": "true" },
            "apply": { "model": "backrooms:block/pipe/side", "x": 270, "uvlock": false }
        },
		
        {   "when": { "west": "true" },
            "apply": { "model": "backrooms:block/pipe/side", "y": 270, "uvlock": false }
        },
		
        {   "when": { "east": "true" },
            "apply": { "model": "backrooms:block/pipe/side", "y": 90, "uvlock": false }
        },
		
        {   "when": { "north": "true" },
            "apply": { "model": "backrooms:block/pipe/side", "uvlock": false }
        },
	
        {   "when": { "south": "true" },
            "apply": { "model": "backrooms:block/pipe/side", "y": 180, "uvlock": false }
        }
    ]
}
